		}

	public static void fnx(int password, String auth) {
		System.out.println("this function knows password " + password + " and is " + auth);
		int ans = anotherfnx(5);
		System.out.println(ans);
	}
	
	public static int anotherfnx(int repeat) {
		int x = repeat;
		repeat += 5;
		return repeat;
	}
