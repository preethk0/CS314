	public static int digitSum(int number) {
		int sum = 0;
		int ans = 0;
		while(ans != number) {
			ans = number % 10;
			number = number / 10;
			sum = sum + ans;
		}
		return sum;
	}
	
	public static int anotherfnx(int repeat) {
		int x = repeat;
		repeat += 5;
		return repeat;
	}
}
