package introUT;
import java.util.Scanner;
import java.util.Random;

public class Test {
	public static final int WOWGLOBAL = 88;
	public static void main(String[] args) {
		Scanner console = new Scanner(System.in);
		Random rand = new Random();
//		System.out.println("please enter two numbers");
//		int num1 = console.nextInt();
//		int num2 = console.nextInt();
//		System.out.printf("the prod of your numbers is %d \n", num1*num2);
		int sum = 0;
		int count = 0;
		while(sum!=7) {
			int sample = rand.nextInt(6) + 1;
			int samp2 = rand.nextInt(6) + 1; 
			sum = sample + samp2;
			System.out.println(sample + " + " + samp2 + " = " + sum);
			count++;
		}
		System.out.println("Nice you hit 7 after " + count + " tries!");
		double x = 19/5;
		int xmod = (int) x;
		//System.out.println(xmod);
		}

	public static void fnx(int password, String auth) {
		System.out.println("this function knows password " + password + " and is " + auth);
		int ans = anotherfnx(5);
		System.out.println(ans);
	}
	
	public static int anotherfnx(int repeat) {
		int x = repeat;
		repeat += 5;
		return repeat;
	}
}
